SCREEN s_status_kontakte()
{

  ftextout("Status Kontakte",1,0,1,0);
  ftextout("",3,0,0,0);

  LINE("Status Tuerkontakte","")
  {
    ftextout("Status Tuerkontakte",1,0,0,0);

    // Türkontakt Fahrer
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;TUERKONTAKT_FA","");
    INPAapiCheckJobStatus("OKAY");
    INPAapiResultText(text_var,"STAT_TUERKONTAKT_FA_NR_TEXT",1,"");
    INPAapiResultInt(ganz_zahl,"STAT_TUERKONTAKT_FA_NR",1);

    if (ganz_zahl == 2)
    	digitalout(FALSE, 3,0,"Fahrertuer vorne offen","Fahrertuer vorne offen");
    else if (ganz_zahl == 4)	 
    	digitalout(TRUE, 3,0,"Fahrertuer vorne offen","Fahrertuer vorne offen");
    else
    	ftextout("TK Fahrer: " + text_var,3,0,0,1);
    
    // Türkontakt Beifahrer	
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;TUERKONTAKT_BF","");
    INPAapiCheckJobStatus("OKAY");
    INPAapiResultText(text_var,"STAT_TUERKONTAKT_BF_NR_TEXT",1,"");
    INPAapiResultInt(ganz_zahl,"STAT_TUERKONTAKT_BF_NR",1);

    if (ganz_zahl == 2)
    	digitalout(FALSE, 3,35,"Beifahrertuer vorne offen","Beifahrertuer vorne offen");
    else if (ganz_zahl == 4)	 
    	digitalout(TRUE, 3,35,"Beifahrertuer vorne offen","Beifahrertuer vorne offen");
    else
    	ftextout("TK Beifahrer: " + text_var,3,35,0,1);

    // Türkontakt Fahrer hinten
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;TUERKONTAKT_FAH","");
    INPAapiCheckJobStatus("OKAY");
    INPAapiResultText(text_var,"STAT_TUERKONTAKT_FAH_NR_TEXT",1,"");
    INPAapiResultInt(ganz_zahl,"STAT_TUERKONTAKT_FAH_NR",1);

    if (ganz_zahl == 2)
    	digitalout(FALSE, 6,0,"Fahrertuer hinten offen","Fahrertuer hinten offen");
    else if (ganz_zahl == 4)	 
    	digitalout(TRUE, 6,0,"Fahrertuer hinten offen","Fahrertuer hinten offen");
    else
    	ftextout("TK Fahrer hinten: " + text_var,6,0,0,1);

    // Türkontakt Beifahrer hinten
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;TUERKONTAKT_BFH","");
    INPAapiCheckJobStatus("OKAY");
    INPAapiResultText(text_var,"STAT_TUERKONTAKT_BFH_NR_TEXT",1,"");
    INPAapiResultInt(ganz_zahl,"STAT_TUERKONTAKT_BFH_NR",1);

    if (ganz_zahl == 2)
    	digitalout(FALSE, 6,35,"Beifahrertuer  hinten offen","Beifahrertuer  hinten offen");
    else if (ganz_zahl == 4)	 
    	digitalout(TRUE, 6,35,"Beifahrertuer  hinten offen","Beifahrertuer  hinten offen");
    else
    	ftextout("TK Beifahrer hinten: " + text_var,6,35,0,1);

  }
  LINE("Schlossnuesse","")
  {

    ftextout("Schaltnuss Hallsensoren",1,0,0,0);

    // Schaltnuss Hallsensor 1
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;SCHALTNUSS_HALLSENSOREN","");
    INPAapiCheckJobStatus("OKAY");
    INPAapiResultText(text_var,"STAT_HALL1_AKTIV_TEXT",1,"");
    INPAapiResultInt(ganz_zahl,"STAT_HALL1_AKTIV",1);

    if (ganz_zahl == 2)
    	digitalout(TRUE, 3,0,"Hallsensor 1 aktiv","Hallsensor 1 aktiv");
    else if (ganz_zahl == 4)	 
    	digitalout(FALSE, 3,0,"Hallsensor 1 aktiv","Hallsensor 1 aktiv");
    else
    	ftextout("Hallsensor 1: " + text_var,3,0,0,1);
    	
    // Schaltnuss Hallsensor 2
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;SCHALTNUSS_HALLSENSOREN","");
    INPAapiCheckJobStatus("OKAY");
    INPAapiResultText(text_var,"STAT_HALL2_AKTIV_TEXT",1,"");
    INPAapiResultInt(ganz_zahl,"STAT_HALL2_AKTIV",1);

    if (ganz_zahl == 2)
    	digitalout(TRUE, 6,0,"Hallsensor 2 aktiv","Hallsensor 2 aktiv");
    else if (ganz_zahl == 4)	 
    	digitalout(FALSE, 6,0,"Hallsensor 2 aktiv","Hallsensor 2 aktiv");
    else
    	ftextout("Hallsensor 2: " + text_var,6,0,0,1);    	


    // Stellung Schaltnuss
    ftextout("Schaltnuss Stellung",1,50,0,0);
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;SCHALTNUSS_STELLUNG","");
    INPAapiCheckJobStatus("OKAY");
    	

{
  ftextout("Steuern Spiegel",1,0,1,0);
  ftextout("",3,0,0,0);
  LINE("","")
  {
    
    ftextout("< F1 >  Spiegelselbsttest aktivieren"       , 4,5,0,1);
    ftextout("< F2 >  Spiegelheizung einschalten"         , 6,5,0,1);
    ftextout("< F3 >  Spiegelheizung ausschalten"         , 8,5,0,1);
    ftextout("< F4 >  Spiegel abblenden"                  ,10,5,0,1);
    ftextout("< F5 >  Spiegel aufhellen"                  ,12,5,0,1);
    ftextout("< F6 >  Spiegel einklappen"     	      ,14,5,0,1);
    ftextout("< F7 >  Spiegel ausklappen"                 ,16,5,0,1);

    ftextout("",7,0,0,0);
  }

  LINE("","")
  {

      INPAapiJob(sgbd,"STATUS_LESEN","ARG;AUSSENSPIEGEL_HEIZUNG","");
      INPAapiCheckJobStatus("OKAY");

      INPAapiResultDigital(logik_var,"STAT_SPIEGEL_HEIZUNG_EIN",1);
      digitalout(logik_var, 1,5,"Spiegelheizung","Spiegelheizung");

      INPAapiJob(sgbd,"STATUS_LESEN","ARG;AUSSENSPIEGEL_HEIZUNG","");
      INPAapiCheckJobStatus("OKAY");


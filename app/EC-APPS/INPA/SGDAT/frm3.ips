MENU m_steuern()
{
  INIT {
    setmenutitle("Steuern");

	
    // Ansteuerung Spiegel, nur wenn auch verbaut
    if (LBLinSpiegel == TRUE)
        setitem(3,"Spiegel",TRUE);
    else
        setitem(3,"Spiegel",FALSE);

  }
  ITEM( 1 ,"Init. Fenster.")
  {
    setmenu(m_steuern_init_fh);
    setscreen( s_steuern_init_fh ,TRUE);
  }
  ITEM( 2 ,"FH verfahren")
  {
    setmenu(m_steuern_fh_verfahren);
    setscreen( s_steuern_fh_verfahren ,TRUE);
  }

  ITEM( 3 ,"Spiegel")
  {
    setmenu(m_steuern_spiegel);
    setscreen( s_steuern_spiegel ,TRUE);
  }
  ITEM( 4 ,"ALC")
  {
        setmenu(m_steuern_ahl);
        setscreen( s_steuern_ahl ,TRUE);
  }

  ITEM( 5 ,"Lampen")
  {
        setmenu(m_steuern_lampen);
        setscreen( s_steuern_lampen ,TRUE);
  }
  ITEM( 6 ,"ELSV")
  {
        setmenu(m_steuern_elsv);
        setscreen( s_steuern_elsv ,TRUE);
  }

  ITEM( 7 ,"Lichtüberw.")
  {
    	INPAapiJob(sgbd,"STEUERN","ARG;STEUERN_KALTLICHTUEBERWACHUNG_PER_DIAGNOSE;1","");
    	INPAapiCheckJobStatus("OKAY");

	INPAapiJob(sgbd,"STEUERN","ARG;STEUERN_WARMLICHTUEBERWACHUNG;1","");
    	INPAapiCheckJobStatus("OKAY");

  }
  ITEM(18 ,"Gesamt")  {
    start();
    deselect();
  }
  ITEM( 9 ,"Druck")  {
    printscreen();
  }
  ITEM( 10 ,"Zurück")  {
    setscreen( s_main ,TRUE);
    setmenu( m_main );
  }

  ITEM( 11 ,"Logger löschen")
  {
      INPAapiJob(sgbd,"STEUERN_FH_LOGGER_REVERSIERER_LOESCHEN","","");
      INPAapiCheckJobStatus("OKAY");

      INPAapiJob(sgbd,"STEUERN_FH_LOGGER_DENORMIERER_LOESCHEN","","");
      INPAapiCheckJobStatus("OKAY");


  }



  ITEM( 20 ,"Exit")  {
    exit();
  }
}

// *************************************************
// *** Steuernmenü Fenster Initialisierung       ***
// *************************************************
MENU m_steuern_init_fh()
{
int    input_state;  // Eingabestatus
int    input_ok=0;   // Eingabe  mit OK beendet
  INIT {
    setmenutitle("Einlernen / Denormieren Fenster");
  }

  ITEM( 1 ,"Init alle")
  {
    inputdigital(logik_var,"Fensterinitialisierung  alle"
                          ,"     !!! K E I N Einklemmschutz !!!" +CRLF+
                           ""+CRLF+
                          "Auswahl über Cursortasten:","Jetzt durchführen.", "Vorgang abbrechen.");
    getinputstate(input_state);
    if ((input_state==input_ok) && (!logik_var))
    {
      // Die hinteren Fenster werden über die Junction-Box eingelernt
      INPAapiJob(sgbd2,"STEUERN_ROUTINE","ARG;FH_EINLERNEN;STR;0x22","");
      INPAapiCheckJobStatus("OKAY");

      // Die vorderen Fenster werden über das Fussraum-Modul eingelernt
      INPAapiJob(sgbd,"STEUERN_ROUTINE","ARG;FH_EINLERNEN;STR;0x21","");
      INPAapiCheckJobStatus("OKAY");
    }
  }
  ITEM( 2 ,"Init vorne")
  {
    inputdigital(logik_var,"Fensterinitialisierung vorne"
                          ,"     !!! K E I N Einklemmschutz !!!" +CRLF+
                           ""+CRLF+
                          "Auswahl über Cursortasten:","Jetzt durchführen.", "Vorgang abbrechen.");
    getinputstate(input_state);
    if ((input_state==input_ok) && (!logik_var))
    {
      // Die vorderen Fenster werden über das Fussraum-Modul eingelernt
      INPAapiJob(sgbd,"STEUERN_ROUTINE","ARG;FH_EINLERNEN;STR;0x21","");
      INPAapiCheckJobStatus("OKAY");
    }
  }
  ITEM( 3 ,"Init hinten")
  {
    inputdigital(logik_var,"Fensterinitialisierung vorne"
                          ,"     !!! K E I N Einklemmschutz !!!" +CRLF+
                           ""+CRLF+
                          "Auswahl über Cursortasten:","Jetzt durchführen.", "Vorgang abbrechen.");
    getinputstate(input_state);
    if ((input_state==input_ok) && (!logik_var))
    {
      // Die hinteren Fenster werden über die Junction-Box eingelernt
      INPAapiJob(sgbd2,"STEUERN_ROUTINE","ARG;FH_EINLERNEN;STR;0x22","");
      INPAapiCheckJobStatus("OKAY");  
    }
  }
  ITEM( 4 ,"Denor. alle")
  {
    // Die hinteren Fenster werden über die Junction-Box denormiert
    INPAapiJob(sgbd2,"STEUERN_ROUTINE","ARG;FH_DENORMIEREN;STR;0x22","");
    INPAapiCheckJobStatus("OKAY");

    // Die vorderen Fenster werden über das Fussraum-Modul denormiert
    INPAapiJob(sgbd,"STEUERN_ROUTINE","ARG;FH_DENORMIEREN;STR;0x21","");
    INPAapiCheckJobStatus("OKAY");

  }
  ITEM( 5 ,"Denor. vorne")
  {
    // Die vorderen Fenster werden über das Fussraum-Modul denormiert
    INPAapiJob(sgbd,"STEUERN_ROUTINE","ARG;FH_DENORMIEREN;STR;0x21","");
    INPAapiCheckJobStatus("OKAY");
  }
  ITEM( 6 ,"Denor. hinten")
  {
    // Die hinteren Fenster werden über die Junction-Box denormiert
    INPAapiJob(sgbd2,"STEUERN_ROUTINE","ARG;FH_DENORMIEREN;STR;0x22","");
    INPAapiCheckJobStatus("OKAY");
  }
  ITEM( 7 ,"")
  {

  }
  ITEM( 8 ,"")
  {

  }
  ITEM(18 ,"Gesamt")  {
    start();
    deselect();
  }
  ITEM( 9 ,"Druck")  {
    printscreen();
  }
  ITEM( 10 ,"Zurück")  {
    setscreen( s_steuern ,TRUE);
    setmenu( m_steuern );
  }
  ITEM( 20 ,"Exit")  {
    exit();
  }
}


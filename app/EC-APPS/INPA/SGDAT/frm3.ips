// ****************************************************
// ***             S c r e e n a u f b a u          ***
// ****************************************************
// **************************
// *** Statusscreen       ***
// **************************
SCREEN s_status()
{
  ftextout("Status lesen",1,0,1,0);
  ftextout("",3,0,0,0);
  LINE("","")
  {
    ftextout("< F1 >  Fensterheber"      	   , 4,5,0,1);
    ftextout("< F2 >  Taster Fensterheber"     , 6,5,0,1);
    ftextout("< F3 >  Taster Licht"            , 8,5,0,1);
    ftextout("< F4 >  Taster FAS /Sitze" 	   ,10,5,0,1);
    ftextout("< F5 >  Kontakte"     		   ,12,5,0,1);
    ftextout("< F6 >  Licht"     		   ,14,5,0,1);


    // Spiegel	  
    if (LBLinSpiegel == TRUE)
        ftextout("< F7 >  Spiegel"                                   ,16,5,0,1);
    else
        ftextout("< F7 >  Spiegel (nicht über FRM)"             ,16,5,0,1);

    if (LBELSV == TRUE)	
  	ftextout("< F8 >  ELSV (Elektrische Lenksäulenverstellung)"     ,18,5,0,1);
    
    ftextout("< F9 >  Bildschirmdruck"          ,20,5,0,1);
    ftextout("< F10>  Ende"                     ,22,5,0,1);

    ftextout("<Shift> + < F1 >  Denormier-Logger"      , 4,45,0,1);
    ftextout("<Shift> + < F2 >  Reversier-Logger"      , 6,45,0,1);

    //ftextout("<Shift> + < F4 >  Klemmen"          ,10,45,0,1);
    //ftextout("<Shift> + < F5 >  Lampen Digital"      ,12,45,0,1); 

    ftextout("<Shift> + < F4 >  Lampen Digital"         ,10,45,0,1);
    ftextout("<Shift> + < F10>  Exit"           ,22,45,0,1);
  }
}



getAusstattung()
{

    // LIN-Spiegel
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;AUSSENSPIEGEL_LIN","");
    INPAapiCheckJobStatus("OKAY");
    INPAapiResultDigital(logik_var,"STAT_VORHANDEN_LIN_SPIEGEL_EIN",1);

    if (logik_var == TRUE)
        LBLinSpiegel = TRUE;
    else
        LBLinSpiegel = FALSE;

    // Heckrollo
    INPAapiJob(sgbd2,"STATUS_LESEN","ARG;ROLLO_HECK_VORHANDEN","");
    INPAapiCheckJobStatus("OKAY");
    INPAapiResultDigital(logik_var,"STAT_VORHANDEN_HECKROLLO_EIN",1);

    if (logik_var == TRUE)
        LBHeckrollo = TRUE;
    else
        LBHeckrollo = FALSE;        

    // ELSV
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;ELSV_VORHANDEN","");
    INPAapiCheckJobStatus("OKAY");
    INPAapiResultDigital(logik_var,"STAT_VORHANDEN_ELSV_EIN",1);

    if (logik_var == TRUE)
        LBELSV = TRUE;
    else
        LBELSV = FALSE;                
     
    
    // Spiegel beiklappen
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;AUSSENSPIEGEL_KLAPPEN_VORHANDEN","");
    INPAapiCheckJobStatus("OKAY");
    INPAapiResultDigital(logik_var,"STAT_VORHANDEN_SPIEGEL_BEIKLAPPEN_EIN",1);

    if (logik_var == TRUE)
        LBBeiklappen = TRUE;
    else
        LBBeiklappen = FALSE;                
     
    // Spiegel abblenden
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;AUSSENSPIEGEL_ABBLENDEN_VORHANDEN","");
    INPAapiCheckJobStatus("OKAY");
    INPAapiResultDigital(logik_var,"STAT_VORHANDEN_SPIEGEL_ABBLENDEN_EIN",1);

    if (logik_var == TRUE)
        LBAbblenden = TRUE;
    else
        LBAbblenden = FALSE;      
        
        
    // Sitzbedienfunktionen
 
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;SITZMASSAGE_FRONT_BEDIENEINHEIT","");
    INPAapiCheckJobStatus("OKAY");
    INPAapiResultDigital(logik_var,"STAT_SITZMASSAGE_FRONT_VORHANDEN",1);

    if (logik_var == TRUE)
        LBSMassagVorne = TRUE;
    else
        LBSMassagVorne = FALSE;  


    // Taster Sitzmassage hinten
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;SITZMEMORY_FRONT_BEDIENEINHEIT","");
    //INPAapiCheckJobStatus("OKAY");
    INPAapiResultDigital(logik_var,"STAT_SITZMEMORY_FRONT_VORHANDEN",1);

    if (logik_var == TRUE)
        LBSMemVorne = TRUE;
    else
        LBSMemVorne = FALSE;  
              
        
}




    INPAapiResultDigital(logik_var,"STAT_SCHALTNUSS_STELLUNG_ENTRIEGELN_EIN",1);
    digitalout(logik_var, 6,40,"entriegeln","entriegeln");

    INPAapiResultDigital(logik_var,"STAT_SCHALTNUSS_STELLUNG_SICHERN_EIN",1);
    digitalout(logik_var, 6,60,"sichern","sichern");

    INPAapiResultDigital(logik_var,"STAT_SCHALTNUSS_STELLUNG_UNGUELTIG_EIN",1);
    digitalout(logik_var, 8,35,"ungueltig","ungueltig");

  }

  LINE("RFS/BLS","")
  {
	
    ftextout("Bremslichtschalter / Rückfahrlichtschalter",1,0,0,0);	

    INPAapiJob(sgbd,"STATUS_LESEN","ARG;BREMSLICHTSCHALTER","");

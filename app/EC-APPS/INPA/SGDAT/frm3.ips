// *** Steuernmenü Fenster verfahren             ***
// *************************************************
MENU m_steuern_fh_verfahren()
{
  INIT {
    setmenutitle("Verfahren Fenster");
 
  }
  ITEM( 1 ,"Schliess. alle")
  {
    
    	// Die hinteren Fenster werden über die Junction-Box verfahren
    	INPAapiJob(sgbd2,"STEUERN_ROUTINE","ARG;FH_VERFAHREN;str;0x22;0x04;1","");
    	INPAapiCheckJobStatus("OKAY");

    	// Die vorderen Fenster werden über das Fussraum-Modul verfahren
    	INPAapiJob(sgbd,"STEUERN_ROUTINE","ARG;FH_VERFAHREN;str;0x21;0x04;1","");
    	INPAapiCheckJobStatus("OKAY");
 
  }
  ITEM( 2 ,"Schliess. vorne")
  {
    	// Die vorderen Fenster werden über das Fussraum-Modul verfahren
    	INPAapiJob(sgbd,"STEUERN_ROUTINE","ARG;FH_VERFAHREN;str;0x21;0x04;1","");
    	INPAapiCheckJobStatus("OKAY");

  }
  ITEM( 3 ,"Schliess. hinten")
  {

    // Die hinteren Fenster werden über die Junction-Box verfahren
    INPAapiJob(sgbd2,"STEUERN_ROUTINE","ARG;FH_VERFAHREN;str;0x22;0x04;1","");
    INPAapiCheckJobStatus("OKAY");
  }
  ITEM( 4 ,"Öffnen alle")
  {

    // Die hinteren Fenster werden über die Junction-Box verfahren
    INPAapiJob(sgbd2,"STEUERN_ROUTINE","ARG;FH_VERFAHREN;str;0x22;0x03;1","");
    INPAapiCheckJobStatus("OKAY");

    // Die vorderen Fenster werden über das Fussraum-Modul verfahren
    INPAapiJob(sgbd,"STEUERN_ROUTINE","ARG;FH_VERFAHREN;str;0x21;0x03;1","");
    INPAapiCheckJobStatus("OKAY");

  }
  ITEM( 5 ,"Öffnen vorne")
  {
    // Die vorderen Fenster werden über das Fussraum-Modul verfahren
    INPAapiJob(sgbd,"STEUERN_ROUTINE","ARG;FH_VERFAHREN;str;0x21;0x03;1","");
    INPAapiCheckJobStatus("OKAY");
  }
  ITEM( 6 ,"Öffnen hinten")
  {
    // Die hinteren Fenster werden über die Junction-Box verfahren
    INPAapiJob(sgbd2,"STEUERN_ROUTINE","ARG;FH_VERFAHREN;str;0x22;0x03;1","");
    INPAapiCheckJobStatus("OKAY");
  }
  ITEM( 7 ,"")
  {

  }
  ITEM( 8 ,"")
  {

  }
  ITEM(18 ,"Gesamt")  {
    start();
    deselect();
  }
  ITEM( 9 ,"Druck")  {
    printscreen();
  }
  ITEM( 10 ,"Zurück")  {
    setscreen( s_steuern ,TRUE);
    setmenu( m_steuern );
  }
  ITEM( 20 ,"Exit")  {
    exit();
  }
}
// *************************************************
// *** Steuernmenü Spiegel                       ***
// *************************************************
MENU m_steuern_spiegel()
{
  INIT {
    setmenutitle("Ansteuerung Spiegel");

    // Abblenden des Spiegels nur, wenn auch abblenden möglich
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;AUSENSPIEGEL_ABBLENDEN_VORHANDEN","");
    INPAapiCheckJobStatus("OKAY");

    INPAapiResultDigital(logik_var,"STAT_VORHANDEN_SPIEGEL_ABBLENDEN_EIN",1);
    if (logik_var == TRUE)
    {
        setitem(4,"Abblenden",TRUE);
        setitem(5,"Aufhellen",TRUE);
    }
    else
    {
        setitem(4,"Abblenden",FALSE);
        setitem(5,"Aufhellen",FALSE);
    }
    
    // Klappen des Spiegels nur, wenn auch klappen möglich
    INPAapiJob(sgbd,"STATUS_LESEN","ARG;AUSSENSPIEGEL_KLAPPEN_VORHANDEN","");
    INPAapiCheckJobStatus("OKAY");

    INPAapiResultDigital(logik_var,"STAT_VORHANDEN_SPIEGEL_BEIKLAPPEN_EIN",1);
    if (logik_var == TRUE)
    {
        setitem(6,"Einklappen",TRUE);
        setitem(7,"Ausklappen",TRUE);
    }
    else
    {
        setitem(6,"Einklappen",FALSE);
        setitem(7,"Ausklappen",FALSE);
    }
        
  }

  ITEM( 1 ,"Selbsttest")
  {

    INPAapiJob(sgbd,"STEUERN_ROUTINE","ARG;AUSSENSPIEGEL_SELBSTTEST;STR","");
    INPAapiCheckJobStatus("OKAY");

  }  
  ITEM( 2 ,"Heizung ein")
  {

   //Einschalten der Spiegelheizung
    INPAapiJob(sgbd,"STEUERN","ARG;AUSSENSPIEGEL_HEIZUNG;0x05;240","");
    INPAapiCheckJobStatus("OKAY");

  }
  ITEM( 3 ,"Heizung aus")
  {
   //Ausschalten der Spiegelheizung
    INPAapiJob(sgbd,"STEUERN","ARG;AUSSENSPIEGEL_HEIZUNG;0x00;240","");
    INPAapiCheckJobStatus("OKAY");
  }
  ITEM( 4 ,"Abblenden")
  {
   //Abblenden 
    INPAapiJob(sgbd,"STEUERN","ARG;AUSSENSPIEGEL_ABBLENDEN;0x01","");
    INPAapiCheckJobStatus("OKAY");

  }
  ITEM( 5 ,"Aufhellen")
  {
   //Aufhellen
    INPAapiJob(sgbd,"STEUERN","ARG;AUSSENSPIEGEL_ABBLENDEN;0x00","");
    INPAapiCheckJobStatus("OKAY");

  }
  ITEM( 6 ,"Einklappen") 
  {
    INPAapiJob(sgbd,"STEUERN","ARG;AUSSENSPIEGEL_KLAPPEN;0x01","");
    INPAapiCheckJobStatus("OKAY");

  }
  ITEM( 7 ,"Ausklappen")
  {
    INPAapiJob(sgbd,"STEUERN","ARG;AUSSENSPIEGEL_KLAPPEN;0x02","");
    INPAapiCheckJobStatus("OKAY");

  }

  ITEM( 8 ,"")
  {

  }
  ITEM(18 ,"Gesamt")  {
    start();
    deselect();
  }
  ITEM( 9 ,"Druck")  {
    printscreen();
  }
  ITEM( 10 ,"Zurück")  {
    setscreen( s_steuern ,TRUE);
    setmenu( m_steuern );
  }
  ITEM( 20 ,"Exit")  {
    exit();
  }
}
